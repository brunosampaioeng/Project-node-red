    {
        "id": "check_cep_query",
        "type": "function",
        "z": "fa530b25641010ca",
        "name": "Check CEP Query",
        "func": "var cep = msg.req.query.cep || '';\n\n// Se n√£o houver CEP no query, apenas mostra o formul√°rio\nif (!cep || cep.trim() === '') {\n    msg.payload = '<h2>Busca de CEP</h2><p>Digite um CEP no formul√°rio abaixo para buscar.</p>';\n    return [null, msg];\n}\n\n// Se houver CEP, prepara para buscar\ncep = cep.replace(/\\D/g, '');\nif (!/^[0-9]{8}$/.test(cep)) {\n    msg.payload = '<h2>CEP inv√°lido</h2><p>O CEP deve conter exatamente 8 n√∫meros.</p>';\n    return [null, msg];\n}\n\n// Define o CEP como par√¢metro para a busca\nmsg.req.params = msg.req.params || {};\nmsg.req.params.cep = cep;\nreturn [msg, null];",
